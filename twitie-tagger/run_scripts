# $1 = input untagged corpus $2 = output, POS-tagged corpus
java -jar twitie_tag.jar models/gate-EN-twitter.model corpora/untagged/kevinhart4real_tweets.txt > corpora/tagged/kevinhart4real_tweets_tagged.txt
